<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Kesehatan">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <FrameLayout
            android:id="@+id/navbar"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="@color/sky">
            <ImageButton
                android:id="@+id/back_kesehatan"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/arrow_back"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp">
            </ImageButton>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Toko Kesehatan"
                android:layout_marginTop="30dp"
                android:textStyle="bold"
                android:textSize="20dp"
                android:textColor="@color/black"
                android:layout_marginLeft="60dp">
            </TextView>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/alamat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/navbar">
            <ImageView
                android:id="@+id/loc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/location"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="30dp">
            </ImageView>
            <TextView
                android:id="@+id/almt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Alamat"
                android:textSize="13sp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="15dp">
            </TextView>
            <ImageView
                android:id="@+id/edit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/logo_edit"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="320dp">
            </ImageView>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/search_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/alamat">
            <EditText
                android:id="@+id/konfirmasi"
                android:layout_width="308dp"
                android:layout_height="38dp"
                android:background="@drawable/kotak_abumuda"
                android:layout_below="@id/almt"
                android:layout_marginTop="15dp"
                android:hint="   Dr Dimas Mulyo spesialis Kandungan"
                android:textSize="15dp"
                android:textStyle="italic"
                android:padding="10dp"
                android:layout_marginLeft="30dp"
                android:drawableStart="@drawable/logo_search">
            </EditText>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/item_kesehatan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/search_bar"
            android:layout_marginStart="30dp">
            <TextView
                android:id="@+id/srcbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Obat - Obatan"
                android:textSize="13sp"
                android:textColor="@color/black"
                android:layout_marginTop="20dp"
                android:textStyle="bold"
                android:layout_below="@id/konfirmasi">
            </TextView>
            <LinearLayout
                android:id="@+id/utm"
                android:layout_width="139dp"
                android:layout_height="217dp"
                android:background="@color/grey"
                android:layout_below="@id/srcbar"
                android:layout_marginTop="40dp"
                android:orientation="vertical">
                <Button
                    android:id="@+id/rec"
                    android:layout_width="104dp"
                    android:layout_height="116dp"
                    android:layout_below="@id/srcbar"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="17dp"
                    android:backgroundTint="@color/white">
                </Button>
                <TextView
                    android:id="@+id/txt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Paramex 1 pax           Rp. 9.000"
                    android:textStyle="bold"
                    android:textSize="14sp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp"
                    android:textColor="@color/black">
                </TextView>
                <Button
                    android:id="@+id/pnct"
                    android:layout_width="104dp"
                    android:layout_height="40dp"
                    android:backgroundTint="@color/white"
                    android:textColor="@color/black"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="20dp"
                    android:text="Tambah"
                    android:textSize="13dp">
                </Button>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/utm2"
                android:layout_width="139dp"
                android:layout_height="217dp"
                android:background="@color/grey"
                android:layout_below="@id/srcbar"
                android:layout_marginTop="40dp"
                android:layout_marginLeft="160dp"
                android:orientation="vertical">
                <Button
                    android:id="@+id/rec2"
                    android:layout_width="104dp"
                    android:layout_height="116dp"
                    android:backgroundTint="@color/white"
                    android:layout_below="@id/srcbar"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="17dp">
                </Button>
                <TextView
                    android:id="@+id/txt2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Nama produk Harga"
                    android:textStyle="bold"
                    android:textSize="14sp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp"
                    android:textColor="@color/black">
                </TextView>
                <Button
                    android:id="@+id/pnct2"
                    android:layout_width="104dp"
                    android:layout_height="40dp"
                    android:backgroundTint="@color/white"
                    android:textColor="@color/black"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="20dp"
                    android:text="Tambah"
                    android:textSize="13dp">
                </Button>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/utm3"
                android:layout_width="139dp"
                android:layout_height="217dp"
                android:background="@color/grey"
                android:layout_below="@id/srcbar"
                android:layout_marginTop="280dp"
                android:orientation="vertical">
                <Button
                    android:id="@+id/rec3"
                    android:layout_width="104dp"
                    android:layout_height="116dp"
                    android:backgroundTint="@color/white"
                    android:layout_below="@id/srcbar"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="17dp">
                </Button>
                <TextView
                    android:id="@+id/txt3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Nama produk Harga"
                    android:textStyle="bold"
                    android:textSize="14sp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp"
                    android:textColor="@color/black">
                </TextView>
                <Button
                    android:id="@+id/pnct3"
                    android:layout_width="104dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="3dp"
                    android:backgroundTint="@color/white"
                    android:text="Tambah"
                    android:textColor="@color/black"
                    android:textSize="13dp">
                </Button>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/utm4"
                android:layout_width="139dp"
                android:layout_height="217dp"
                android:background="@color/grey"
                android:layout_below="@id/srcbar"
                android:layout_marginTop="280dp"
                android:layout_marginLeft="160dp"
                android:orientation="vertical">
                <Button
                    android:id="@+id/rec4"
                    android:layout_width="104dp"
                    android:layout_height="116dp"
                    android:backgroundTint="@color/white"
                    android:layout_below="@id/srcbar"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="17dp">
                </Button>
                <TextView
                    android:id="@+id/txt4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Nama produk Harga"
                    android:textStyle="bold"
                    android:textSize="14sp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp"
                    android:textColor="@color/black">
                </TextView>
                <Button
                    android:id="@+id/pnct4"
                    android:layout_width="104dp"
                    android:layout_height="40dp"
                    android:backgroundTint="@color/white"
                    android:textColor="@color/black"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="20dp"
                    android:text="Tambah"
                    android:textSize="13dp">
                </Button>
            </LinearLayout>
        </FrameLayout>


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_kesehatan"
            android:layout_marginTop="47dp">
            <ImageView
                android:id="@+id/bellow"
                android:layout_width="430dp"
                android:layout_height="58dp"
                android:background="@color/sky">
            </ImageView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Jumlah Barang :"
                android:textSize="15sp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="15dp">
            </TextView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Estimasi Harga"
                android:textSize="10sp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:layout_marginLeft="130dp"
                android:layout_marginTop="10dp">
            </TextView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rp.30.000"
                android:textSize="10sp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:layout_marginLeft="130dp"
                android:layout_marginTop="25dp">
            </TextView>
            <Button
                android:id="@+id/pnct5"
                android:layout_width="130dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/white"
                android:textColor="@color/black"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="10dp"
                android:text="Lihat keranjang"
                android:textSize="8dp">
            </Button>

        </FrameLayout>
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>

